{
  "cSpell.customDictionaries": {
    "custom-dictionary-workspace": {
      "name": "custom-dictionary-workspace",
      "path": "${workspaceFolder:dandelion}/.cspell/custom-dictionary-workspace.txt",
      "addWords": true,
      "scope": "workspace"
    }
  },
  "cSpell.ignoreWords": [
    "alpn",
    "apln",
    "getaddrinfo",
    "httparse",
    "iplist",
    "ipnetwork",
    "nexthop",
    "oneshot",
    "rustls",
    "sockaddr",
    "socktype",
    "termcolor",
    "tungstenite",
    "xfer"
  ]
}
